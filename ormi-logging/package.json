{
  "name": "ormi-logging-tutorial",
  "version": "1.0.0",
  "description": "ORMI API Logging & Health Monitoring Tutorial - Production-ready TypeScript implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "ts-node src/test/integration.ts",
    "health-check": "ts-node src/scripts/healthCheck.ts",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ormi",
    "somnia",
    "blockchain",
    "logging",
    "health-monitoring",
    "winston",
    "typescript",
    "api-client"
  ],
  "author": "Somnia Developer",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/somnia-network/ormi-logging-tutorial.git"
  },
  "bugs": {
    "url": "https://github.com/somnia-network/ormi-logging-tutorial/issues"
